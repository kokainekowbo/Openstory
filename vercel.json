{
  "version": 2,
  "name": "openstory",
  "buildCommand": "cd openstory-app && npm install && npm run build",
  "outputDirectory": "openstory-app/build",
  "installCommand": "cd openstory-app && npm install",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 